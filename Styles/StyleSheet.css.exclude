head{margin:0; padding :0}
body
{

    
    font-size:62.5%; 
    margin :0px;
    padding :0px;
    width:100%;
    height :100%
}
/*
#container
{
   min-height:100%;
   position:relative;
}
*/
    
.topcontent
{   /*position:absolute;*/
    margin:0;
    padding:0;
    top:0em;
    /*left:0em;*/
    width :100%;
    background-image:url('~/NewImages/MasterBackGround.jpg');
    background-repeat:no-repeat;
    background-attachment:fixed;
    background-position:center;
}
.topcontent#HeadLogo
{   
   height:25em;
   left:10em;
}

.topcontent.text
{
   position:absolute;
    top:2em; 
    left:50%;
    width:15em;
    /*font-family :Daniel;*/
    font-size:4em;
}

.logOn
{
    position:absolute;
    font-family: Verdana, Helvetica, sans-serif;
    right:5em;
    top:0em;
    height: 1.2em;
    font-size:2em;
}

.LogOnButton {
  background-color:transparent;
  border: 0;
  border-bottom: .01em solid #B22222;
  color: #B22222;
  cursor: pointer;
  font-size:2em;
  /*margin: 0;
  padding: 0;*/
}
.LogOnButton a.hover
{
    color:Blue;
}
#masterbody
{
    position:relative ;
    /*top:24em;*/
    float:right;
    width:100%;
    font-size : 1.5em;
    background-color: White;
    text-indent:1em;
    text-align:left
}
#masterfooter
{
    position:absolute;
    bottom :0em;
    width:100%;
    /*clear:both;*/
    height: 4em;
    background-color:transparent ;
}

#subheader
{
	position:absolute;
	background-color:transparent;
	top:26em;
	left:35%;
	height: 2em;
	padding-left: 2em;
	padding-top: 0em;
    text-align: right;
}

#subheader a
{
    color: #B22222; /*#daa520;*/
    font-family: Verdana, Helvetica, sans-serif;
    text-decoration: none;
    font-size:  2em;
    font-weight: bold;
    line-height: 2em;
    padding-right: 1em;
    text-align: left;
}

#subheader a:hover
{
	color: white;
}

#leftbody
{   
    left :0px;
    width:30%;
    background-color: Yellow;
}



a:link  
{
    border-style: none;
    border-width: thick;
    border-color: Lime;
    text-decoration: none;
    color:  black;
}

a, a:hover 
{
    text-decoration: underline;
    color:  black;
}

a, a:visited 
{
    text-decoration: underline;
    color:  black;
}

a, a:active 
{
    text-decoration: underline;
    color:  black;
}

ul li   
{
    list-style-type:    square;
}

ul li li    
{
    list-style-type:    disc;
}

ul li li li 
{
    list-style-type:    circle;
}

ol li   
{
    list-style-type:    decimal;
}

ol ol li 
{
    list-style-type:    lower-alpha;
}

ol ol ol li
{
    list-style-type:    lower-roman;
}

hr 
{
    height:1pt;
    text-align:left
}

small
{
    font-size:  .08em;
}

big
{
    font-size:  1.4em;
}

blockquote, pre
{
    font-family:    Lucida Console, monospace;
}


#outer
{
	padding: 0em;
	margin: 0em;
}

.addToCart
{
 border-color: Green; 
 background-color: Green;
}

.addToCart a img image
{
 border-color: yellow; 
 border-style: dashed;
 border-width: thick;
}

#header
{
	position: relative;
	background: black url('images/headerbackground.gif') repeat-x bottom left;
	height: 1.2em;
	padding-left: 2em;
}

#header h1
{
	position: absolute;
	top: 4em;
	font-size: xx-large;
}

#header h1 a
{
	color: #fff;
	text-decoration: none;
}

#header h2
{
	position: absolute;
	font-size: 0.8em;
	top: 8em;
	color: #E0E8EF;
}

#menu
{
	position: absolute;
	top: 5em;
	right: 3em;
}

#menu ul
{
	list-style: none;
}

#menu li
{
	display: inline;
	padding-left: 1.1em;
	margin-left: 1em;
	border-left: dotted .01em #E0E8EF;
}

#menu li.first
{
	border: 0em;
	margin-left: 0em;
	padding-left: 0em;
}

#menu li a
{
	color: #fff;
	padding: 0.1em;
	text-decoration: none;
}

#menu li a:hover
{
	text-decoration: underline;
}


#content
{
	line-height: 1.5em;
}


#content p
{
	margin-bottom: 1.5em;
}

#content h2,h3,h4,h5,h6
{
	color: #545454;
	margin-bottom: 1em;
	border-bottom: dashed .01em #dfdfdf;
	padding-bottom: 0.3em;
}

#content h2
{
	font-size: 1.5em;
}

#content h3
{
	font-size: 1.1em;
}

#content ul
{
	margin-bottom: 1.5em;
	padding-left: 1em;
	list-style: square;
}

#content blockquote
{
	margin-bottom: 1.5em;
	padding: 0.75em 0.75em 0.75em 1.5em;
	background: #FDFDFD url('images/a7.gif') top left repeat-x;
}

#content blockquote p
{
	margin-bottom: 0em;
    width: 36.2em;
}

#content table
{
	margin-bottom: 1.5em;
	border-collapse: collapse;
	text-align: left;
}

#content table th
{
	font-weight: bold;
	padding: 0.5em;
}

#content table td
{
	padding: 0.5em;
	border-right: solid 1px #fff;
}

#content table tr.rowH
{
	color: #fff;
	background: #006192 url('images/tableRowH.gif') top left repeat-x;
	border-right: solid .01em #fff;
}

#content table tr.rowA
{
	background: #F8F8F8 url('images/a6.gif') top left repeat-x;
	border-top: solid .01em #fff;
	color: inherit;
}

#content table tr.rowB
{
	background: #FDFDFD url('images/a7.gif') top left repeat-x;
	border-top: solid .01em #fff;
	color: inherit;
}


.clear
{
	clear: both;
}

/* ================================
    CSS STYLES FOR THE COMMERCE STARTER KIT 
    v1.0, 10/2002
   ================================
*/   

.HomeHead
{
    color: #999966;
    font-family: Verdana, Arial;
    font-size: 2em;
    font-weight: bold;
    height: 3.5em;
}

/* */
.ContentHead
{
    background-color: #dddca3;
    color: #696969;
    font-family: Verdana, Arial;
    font-size: 2.em;
    line-height: 2.6em;
    font-weight: bold;
    height: 3.5em; 
    vertical-align: middle; 
    padding-top: .5em;
    padding-left: .5em;
    text-align: left;
}

/* */
.SubContentHead
{
    background-color: #dddca3;
    color: #696969;
    font-family: Verdana, Arial;
    font-size: 1.8em;
    font-weight: bold;
    height: 3.em;
    padding: 0em 10em 0em 10em;
}



/* */
.ErrorText
{
    color: red;
    font-family: Verdana, Arial;
    font-size: 1.2em;
    line-height: 2em
}


/* text style for the links */
A.SiteLink, A.SiteLink:link, A.SiteLink:visited
{
    font-family: Verdana, Helvetica, sans-serif;
    text-decoration: none;
    color: black;
    font-size: 11px;
}   

A.SiteLink:hover    
{
    text-decoration: underline;
    color: white;
}

img {border: none}


A.SiteLinkBold, A.SiteLinkBold:link, A.SiteLinkBold:visited
{
    color: #B22222; /*	  #dddddd;*/
    font-family: Verdana, Helvetica, sans-serif;
    text-decoration: none;
    font-size:  1.1em;
    font-weight: bold;
}

A.SiteLinkBold:hover
{
    color: white;
    text-decoration: none;
}

/* */
A.MenuUnselected, A.MenuUnselected:link, A.MenuUnselected:visited
{
    color: #daa520;
    font-family: Verdana, Helvetica, sans-serif;
    text-decoration: none;
    font-size:  1.2em;
    font-weight: bold;
    line-height: 1.6em;
    padding-left: 1em;
}

A.MenuUnselected:hover
{
    color: white;
    text-decoration: underline;
    padding-left: 1em;
}

A.MenuSelected
{
    color: white;
    font-family: Verdana, Helvetica, sans-serif;
    text-decoration: none;
    font-size:  1.2em;
    font-weight: bold;
    line-height: 1.2em;
    padding-left: 1em
}

A.MenuSelected:hover
{
    color: white;
    font-family: Verdana, Helvetica, sans-serif;
    text-decoration: none;
    padding-left: 1em
}

/* GENERAL */

/* text style used for most text rendered by modules */
.Normal
{
    font-family: Verdana, Helvetica, sans-serif;
    font-size: 1.1em;
    font-weight: normal;
    line-height: 1.2em    
}

.NormalDouble
{
    font-family: Verdana, Helvetica, sans-serif;
    font-size: 1.1em;
    font-weight: normal;
    line-height: 2em;
}

/* text style used for textboxes in the admin pages, for Nav compatibility */
.NormalTextBox
{
    font-family: Verdana, Helvetica, sans-serif;
    font-size: 1.2em;
    font-weight: normal;
}

/* text style used for selects in the admin pages, for Nav compatibility */
.NormalSelect
{
    font-family: Verdana, Helvetica, sans-serif;
    font-size: 1.2em;
    font-weight: normal;
}

/* text style used for textboxes in the edit pages, for Nav compatibility */
.EditTextBox
{
    font-family: Verdana, Helvetica, sans-serif;
    font-size: 1em;
    font-weight: normal;
    border-style: solid;
    border-width: 1px;
    border-color: #999966;
    width: 100%;
}

/* text style used for controls in the admin and edit pages, for Nav compatibility */
.NormalControl
{
    font-family: Verdana, Helvetica, sans-serif;
    font-size: 1.1em;
    font-weight: normal;
}

/* text style used for checkboxes in the admin and edit pages, for Nav compatibility */
.NormalCheckBox
{
    margin-left: -.04em;
    font-family: Verdana, Helvetica, sans-serif;
    font-size: 1.1em;
    font-weight: normal;
}

/* text style used for checkboxes in the admin and edit pages, for Nav compatibility */
.NormalRadio
{
    font-family: Verdana, Helvetica, sans-serif;
    font-size: 1.1em;
    font-weight: normal;
    text-indent: -.05em;
}

/* text style used for checkboxes in the admin and edit pages, for Nav compatibility */
.NormalButton
{
    font-family: Verdana, Helvetica, sans-serif;
    font-size: 1.1em;
    font-weight: normal;
    text-indent: -.05em;
    background-color:#ccccaa;    
}

.NormalRed
{
    font-family: Verdana, Helvetica, sans-serif;
    font-size: 1.1em;
    font-weight: bold;
    color: red
}

.NormalBold
{
    font-family: Verdana, Helvetica, sans-serif;
    font-size: 1.1em;
    font-weight: bold;
    line-height: 1.2em;
    text-align: right;
}

.NormalItalic
{
    font-family: Verdana, Helvetica, sans-serif;
    font-size: 1.1em;
    font-weight: bold;
    line-height: 1.6em    
}

/* FORM ELEMENTS   
----------------------------------------------------------*/

fieldset
{
    margin: 1em 0px;
    padding: 1em;
    border: .1em solid #B22222	; /*#ccc;*/
}

fieldset p 
{
    margin: .02em 1.2em 1em 1em;
}

fieldset.login label, fieldset.register label, fieldset.changePassword label
{
    display: block;
}

fieldset label.inline 
{
    display: inline;
}

fieldset.button
{
  font-family: Verdana, Helvetica, sans-serif;  
  font-size:1em;  
  background-color:transparent;
  border: 0;
  border-bottom: .1em solid #B22222;
  color: #B22222;
  cursor: pointer;
}

fieldset.button a.hover
{
    color:Blue;
}

legend 
{
    font-size: 1.1em;
    font-weight: 600;
    padding: .02em .04em .08em .04em;
}

input.textEntry 
{
    height :2em;
    width: 32em;
    font-size: 1em;
    border: .1em solid #B22222	; /*#ccc;*/
}

input.passwordEntry 
{
    height :2em;
    width: 32em;
    font-size: 1em;    
    border: .1em solid #B22222	;/*#ccc;*/
}

div.accountInfo
{
    width: 50.em;
}

/* MISC  
----------------------------------------------------------*/

.clear
{
    clear: both;
}


.loginDisplay
{
    font-size: 1.1em;
    display: block;
    text-align: right;
    padding: 1emx;
    color: White;
}

.loginDisplay a:link
{
    color: white;
}

.loginDisplay a:visited
{
    color: white;
}

.loginDisplay a:hover
{
    color: white;
}

.failureNotification
{
    font-size: 1.2em;
